{
  "name": "rea-invest-backend",
  "version": "1.0.0",
  "description": "REA INVEST Property Management System Backend",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "rollback": "knex migrate:rollback",
    "make-migration": "knex migrate:make",
    "test": "set NODE_ENV=test && jest",
    "test:watch": "set NODE_ENV=test && jest --watch",
    "test:coverage": "set NODE_ENV=test && jest --coverage",
    "docs:generate": "swagger-jsdoc -d swaggerDef.js './routes/*.js' -o './docs/swagger.json'",
    "docs:serve": "swagger-ui-serve ./docs/swagger.json"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "sharp": "^0.32.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4"
  },
  "main": "index.js",
